{
	"name": "@ctx-core/etsy_cache-ui-svelte",
	"version": "1.0.11",
	"description": "cache data for an etsy account",
	"author": "Brian Takit",
	"homepage": "https://github.com/ctx-core/etsy_cache-ui-svelte#readme",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/cache-etsy.mjs"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.44.0",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.94",
		"@ctx-core/dom": "workspace:^11.0.47",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/etsy_cache": "workspace:^11.0.8",
		"@ctx-core/fetch": "workspace:^11.0.100",
		"@ctx-core/function": "workspace:^20.2.19",
		"@ctx-core/object": "workspace:^22.0.7",
		"@ctx-core/sleep": "workspace:^8.0.31",
		"@ctx-core/store": "workspace:^27.0.54"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-preprocess": "^4.10.0",
		"svelte2tsx": "^0.4.11",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte/issues"
	}
}
