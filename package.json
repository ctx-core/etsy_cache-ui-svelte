{
	"name": "@ctx-core/etsy_cache-ui-svelte",
	"version": "1.4.41",
	"description": "cache data for an etsy account",
	"author": "Brian Takita",
	"homepage": "https://github.com/ctx-core/etsy_cache-ui-svelte#readme",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./Etsy_Store"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.454.0",
		"@ctx-core/dom": "^15.1.23",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/etsy_cache": "^16.1.40",
		"@ctx-core/fetch-undici": "^3.1.38",
		"@ctx-core/function": "^21.22.18",
		"@ctx-core/object": "^31.1.4",
		"@ctx-core/sleep": "^8.2.23",
		"@ctx-core/svelte": "^15.0.7",
		"svelte": "^4.2.7"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.2.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte/issues"
	},
	"sideEffects": false
}