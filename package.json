{
	"name": "@ctx-core/etsy_cache-ui-svelte",
	"version": "1.4.269",
	"description": "cache data for an etsy account",
	"author": "Brian Takita",
	"homepage": "https://github.com/ctx-core/etsy_cache-ui-svelte#readme",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./Etsy_Store"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.529.1",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/etsy_cache": "^16.1.255",
		"@ctx-core/fetch-undici": "^3.1.223",
		"@ctx-core/svelte": "^16.0.102",
		"ctx-core": "^6.5.5",
		"svelte": "^4.2.12"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte/issues"
	},
	"sideEffects": false
}