{
	"name": "@ctx-core/etsy_cache-ui-svelte",
	"version": "1.4.380",
	"description": "cache data for an etsy account",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-etsy_cache-ui-svelte@briantakita.me"
	},
	"homepage": "https://github.com/ctx-core/etsy_cache-ui-svelte#readme",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./Etsy_Store"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.616.0",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/etsy_cache": "^16.1.334",
		"@ctx-core/fetch-undici": "^3.1.293",
		"@ctx-core/svelte": "^16.0.158",
		"ctx-core": "^7.2.28",
		"svelte": "^4.2.18"
	},
	"devDependencies": {
		"c8": "^10.1.2",
		"check-dts": "^0.8.0",
		"tsx": "^4.16.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte/issues"
	},
	"sideEffects": false
}