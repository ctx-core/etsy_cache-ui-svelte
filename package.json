{
	"name": "@ctx-core/etsy_cache-ui-svelte",
	"version": "1.0.14",
	"description": "cache data for an etsy account",
	"author": "Brian Takit",
	"homepage": "https://github.com/ctx-core/etsy_cache-ui-svelte#readme",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-etsy": "./bin/cache-etsy.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/cache-etsy.mjs"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.45.0",
		"@ctx-core/dom": "workspace:^11.0.49",
		"@ctx-core/etsy": "^3.0.19",
		"@ctx-core/etsy_cache": "workspace:^12.0.5",
		"@ctx-core/fetch": "workspace:^11.0.102",
		"@ctx-core/function": "workspace:^20.2.20",
		"@ctx-core/object": "workspace:^22.0.8",
		"@ctx-core/sleep": "workspace:^8.0.32",
		"@ctx-core/svelte": "workspace:^10.1.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.123",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.4.12",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/etsy_cache-ui-svelte/issues"
	}
}